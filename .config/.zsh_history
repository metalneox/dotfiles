    font_family = Fira Semibold\
    position = -100, 160\
    halign = right\
    valign = bottom\
    shadow_passes = 5\
    shadow_size = 10\
}\
\
image {\
    monitor =\
    path = $HOME/.cache/square_wallpaper.png\
